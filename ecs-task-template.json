{
  "containerDefinitions": [
    {
      "name": "finliv-backend",
      "image": "952029690192.dkr.ecr.us-east-1.amazonaws.com/finliv/finliv-backend:%IMAGE_TAG%",
      "portMappings": [
        {
          "containerPort": 9000,
          "hostPort": 9000
        }
      ],
      "cpu": 768,
      "memory": 300,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "ecs_log_group",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "backend"
        }
      },
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "DB_CONNECTION_STRING",
          "value": "mysql://finliv:finlivmasterpassword@finliv-production.c35hf5o8qkta.us-east-1.rds.amazonaws.com:3306/finliv_production"
        },
        {
          "name": "SERVER_PORT",
          "value": "9000"
        },
        {
          "name": "FRONTEND_SERVER",
          "value": "http://finliv-alb-1541941420.us-east-1.elb.amazonaws.com" 
        }
      ]
    }
  ],
  "family": "backend"
}
