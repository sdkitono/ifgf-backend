{
  "containerDefinitions": [
    {
      "name": "db-migrate",
      "image": "952029690192.dkr.ecr.us-east-1.amazonaws.com/finliv/finliv-backend:latest",
      "command": ["sequelize", "db:migrate"],
      "memory": 300,
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
            "awslogs-group": "ecs_log_group",
            "awslogs-region": "us-east-1",
            "awslogs-stream-prefix": "db-migrate"
        }
      },
      "environment": [
        {
          "name": "NODE_ENV",
          "value": "production"
        },
        {
          "name": "DB_CONNECTION_STRING",
          "value": "mysql://finliv:finlivmasterpassword@finliv-production.c35hf5o8qkta.us-east-1.rds.amazonaws.com:3306/finliv_production"
        }
      ]
    }
  ],
  "family": "db-migrate"
}
